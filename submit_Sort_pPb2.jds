executable = do_mSortpPb2.sh
universe   = vanilla
arguments  = $(ArgFile)
log = condor.log
getenv     = True
should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = mSortpPb2.C, Upsilon.h, Style_Kisoo.h
transfer_output_files = Sort_OniaTree_pPb2_PADoubleMuon_$(Process).root
output = log/logSortpPb2/job_$(Process).out
error = log/logSortpPb2/job_$(Process).err
request_disk = 10G
queue ArgFile from Arglist.txt
