executable = do_mCorrel_bkg_pPb.sh
universe   = vanilla
arguments  = $(ArgFile)
log = condor.log
getenv     = True
should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = mCorrel_bkg_pPb.C, Upsilon.h, Style_Kisoo.h
transfer_output_files = deta-dphi_pPb_distribution_bkg_$(Process).root
output = log/logCorBkgpPb/job_$(Process).out
error = log/logCorBkgpPb/job_$(Process).err
queue ArgFile from ArglistCorr.txt
